{
    "name": "godaddy/mwc-shipping",
    "description": "A simple library for easily offering shipping features in WooCommerce",
    "type": "library",
    "license": "proprietary",
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "GoDaddy\\WordPress\\MWC\\Shipping\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GoDaddy\\WordPress\\MWC\\Shipping\\Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:gdcorp-partners/mwc-common.git"
        }
    ],
    "require": {
        "php": "^7.0|^8.0",
        "godaddy/mwc-common": "^3.0.0"
    },
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.1",
        "phpcompatibility/php-compatibility": "^9.3.5",
        "phpunit/phpunit": "^9.5",
        "antecedent/patchwork": "^2.1",
        "10up/wp_mock": "0.4.2"
    }
}
